<view class="maptop">
  <view style="position: absoulute;height: calc(100% - 476rpx);width: 100%;z-index: 3;">
    <map id="map" longitude="{{longitude}}" latitude="{{latitude}}" scale="12" markers="{{markers}}" show-location style="width: 100%; height: 100%;"></map>
  </view>
  <view class="w100 bg-white" style="position:absoulute;bottom:0;padding: 20rpx 34rpx 34rpx;z-index: 2;">
    <view class="text-sm margin-bottom-sm" style="height: 32rpx;">
      <image class="fl margin-right-xs" src="/static/marker.png" style="width: 32rpx;height: 32rpx;" />
      <text class="fl margin-right-sm">其他资源</text>
      <image class="fl margin-right-xs" src="/static/hall.png" style="width: 32rpx;height: 32rpx;" />
      <text class="fl">文明实践站</text>
    </view>
    <view class="flex w100" style="height: 214rpx;">
      <navigator class="flex-sub radius" style="margin-right: 14rpx;height: 95%;" url="./hall">
        <image class="w100" src="/static/service.png" style="height: 100%;"></image>
      </navigator>
      <navigator class="flex-sub radius" style="margin-left: 14rpx;height: 95%;" url="./team">
        <image class="w100" src="/static/team.png" style="height: 100%;"></image>
      </navigator>
    </view>
    <navigator class="radius w100 mt20" style="height: 140rpx;" url="../orderMeal/orderStart">
      <image class="w100" src="/static/diandan.png" style="height: 100%;"></image>
    </navigator>
  </view>
</view>